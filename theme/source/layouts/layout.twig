{% spaceless %}
<!doctype html>
<html lang="de">
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>{{ site.title }} â€“ {{ page.title }}</title>
	<meta name="viewport" content="width=device-width, initial-scale=1">
	{# <meta name="description" content="{{ globals.desc|striptags }}">
	<meta property="og:title" content="{{ globals.page_title|striptags }} | PIU" />
	<meta property="og:description" content="{{ globals.desc|striptags }}" />
